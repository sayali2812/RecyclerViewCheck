<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arlingtonrentacar"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="29" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.arlingtonrentacar.UserProfile" />
        <activity android:name="com.example.arlingtonrentacar.ViewSearchUserActivity" />
        <activity android:name="com.example.arlingtonrentacar.SearchUserActivity" />
        <activity
            android:name="com.example.arlingtonrentacar.RenterHomeScreen"
            android:label="@string/renter_screen" />
        <activity
            android:name="com.example.arlingtonrentacar.AdminHomeScreen"
            android:label="@string/admin_screen" />
        <activity
            android:name="com.example.arlingtonrentacar.ManagerHomeScreen"
            android:label="@string/manager_screen" />
        <activity
            android:name="com.example.arlingtonrentacar.RequestedCarDetailsActivity"
            android:label="View Requested Car Details" />
        <activity
            android:name="com.example.arlingtonrentacar.ViewRequestedCarActivity"
            android:label="@string/viewRequestedCarActivityLabel"
            android:parentActivityName="com.example.arlingtonrentacar.RequestCarActivity" />
        <activity
            android:name="com.example.arlingtonrentacar.RequestCarActivity"
            android:label="@string/requestCarActivityLabel"
            android:parentActivityName="com.example.arlingtonrentacar.RenterHomeScreen" />
        <activity
            android:name="com.example.arlingtonrentacar.RegisterActivity"
            android:label="@string/registerActivityLabel"
            android:parentActivityName="com.example.arlingtonrentacar.MainActivity" />
        <activity
            android:name="com.example.arlingtonrentacar.testactivity"
            android:label="@string/admin_screen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.arlingtonrentacar.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.arlingtonrentacar.manager.View_Reservation_Calendar" />
        <activity android:name="com.example.arlingtonrentacar.manager.View_Reservation_Details" />
    </application>

</manifest>